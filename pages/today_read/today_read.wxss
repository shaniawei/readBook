.today_read_list .item{
  border-bottom: 1px solid #ddd;
  height: 80rpx;
  line-height: 80rpx;
  margin-bottom: 20rpx;
}
.today_read_list .item navigator{
  height: 100%;
}
.today_read_list .item .bookname{
  width: 35%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.today_read_list .item .info{
  font-size: 28rpx;
  width: 65%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.today_read_list .item .info text:nth-child(1){
  color: blue;
}
.today_read_list .item .info text:nth-child(2){
  margin: 0 20rpx;
}